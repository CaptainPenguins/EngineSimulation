FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 5.73e2;

boundaryField
{
	inlet
	{
		type	fixedValue; 
		value	uniform 5.73e2;
	}

	outlet
	{
		type	zeroGradient;
	}

	wall
	{
	type            epsilonWallFunction;
        value           uniform 5.73e2; 
	}

	heatedWall
	{
	        type            epsilonWallFunction;
        value           uniform 5.73e2; 
	}

	frontAndBack
	{
		type	empty;	
	}
}


// ************************************************************************* //
