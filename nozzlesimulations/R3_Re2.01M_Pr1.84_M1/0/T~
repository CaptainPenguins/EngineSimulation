FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 293.15;

boundaryField
{
	inlet
	{
		type	fixedValue;	
		value	uniform 293.15;
	}

	outlet
	{
		type	zeroGradient;
	}

	wall
	{
		type	zeroGradient; 
	}

	heatedWall
	{
		type	fixedGradient; 
		gradient uniform 281439.82; 
	}

	frontAndBack
	{
		type	empty;	
	}
}

